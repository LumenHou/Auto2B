<?xml version="1.0"?>
<root>
	<action>
		<click>
			<moudel>Common</moudel>
			<name>click_comm('$type', elementsMap['$pageName']['$keys'], '$value')</name>
		</click>
		<input>
			<moudel>Common</moudel>
